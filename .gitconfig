[remote "bitnami"]
    url = git@github.com:bitnami/charts.git
    fetch = +refs/heads/*:refs/remotes/bitnami/*
[remote "origin"]
    url = git@github.com:huangyingw/bitnami_charts.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = bitnami
    target = bitnami/master
[merge]
	allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
    ignore = *.lock
    ignore = */deploy.sh
    ignore = readme
[deploy]
	host = localhost
	path = ~/myproject/git/Kubernetes/bitnami/charts
