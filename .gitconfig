[remote "bitnami"]
    url = git@github.com:bitnami/charts.git
    fetch = +refs/heads/*:refs/remotes/bitnami/*
[remote "origin"]
    url = git@github.com:huangyingw/bitnami_charts.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = bitnami
    branch = master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/../bitnami/charts
